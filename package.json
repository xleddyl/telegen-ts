{
  "name": "@xleddyl/telegen-ts",
  "version": "1.0.0",
  "description": "The autogenerated TypeScript library for the official Telegram Bot API",
  "keywords": [
    "telegram",
    "bot",
    "typescript",
    "javascript",
    "library"
  ],
  "author": "xleddyl",
  "license": "ISC",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "scripts": {
    "test": "jest --config jestconfig.json",
    "build": "tsc",
    "translate": "node src/utils/translator.js && prettier --write \"src/core/telegram\"",
    "format": "prettier --write \"src/**/*.ts\" \"src/**/*.js\"",
    "lint": "tslint -p tsconfig.json",
    "prepare": "npm run build",
    "prepublishOnly": "npm test && npm run lint",
    "preversion": "npm run lint",
    "version": "npm run format && git add -A src",
    "postversion": "git push && git push --tags"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/xleddyl/telegrambot-ts.git"
  },
  "bugs": {
    "url": "https://github.com/xleddyl/telegrambot-ts/issues"
  },
  "homepage": "https://github.com/xleddyl/telegrambot-ts#readme",
  "dependencies": {
    "axios": "^1.2.5"
  },
  "devDependencies": {
    "@types/jest": "^29.4.0",
    "jest": "^29.4.1",
    "jsdom": "^21.1.0",
    "prettier": "^2.8.3",
    "ts-jest": "^29.0.5",
    "tslint": "^6.1.3",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^4.9.4"
  },
  "files": [
    "lib/**/*"
  ]
}
